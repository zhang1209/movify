package com.movify.entity;
public class User{
	private Integer uid; // 用户id
	private String uname; // 用户名
	private String upw; // 用户密码
	private String uphoto; // 用户头像
	private String uphone; // 用户手机
	private String uemail; // 用户邮箱
	private String helpcode; // 用户挽救码
	private Integer state; // 用户状态
	private Integer role; // 用户角色外键
	public User(){}
	public User(Integer uid,String uname,String upw,String uphoto,String uphone,String uemail,String helpcode,Integer state,Integer role){
		this.uid=uid;
		this.uname=uname;
		this.upw=upw;
		this.uphoto=uphoto;
		this.uphone=uphone;
		this.uemail=uemail;
		this.helpcode=helpcode;
		this.state=state;
		this.role=role;
	}
	public void setUid(Integer uid){
		this.uid=uid;
	}
	public Integer getUid(){
		return uid;
	}
	public void setUname(String uname){
		this.uname=uname;
	}
	public String getUname(){
		return uname;
	}
	public void setUpw(String upw){
		this.upw=upw;
	}
	public String getUpw(){
		return upw;
	}
	public void setUphoto(String uphoto){
		this.uphoto=uphoto;
	}
	public String getUphoto(){
		return uphoto;
	}
	public void setUphone(String uphone){
		this.uphone=uphone;
	}
	public String getUphone(){
		return uphone;
	}
	public void setUemail(String uemail){
		this.uemail=uemail;
	}
	public String getUemail(){
		return uemail;
	}
	public void setHelpcode(String helpcode){
		this.helpcode=helpcode;
	}
	public String getHelpcode(){
		return helpcode;
	}
	public void setState(Integer state){
		this.state=state;
	}
	public Integer getState(){
		return state;
	}
	public void setRole(Integer role){
		this.role=role;
	}
	public Integer getRole(){
		return role;
	}
}