		this.vdate=vdate;
		this.vtime=vtime;
		this.vrating=vrating;
		this.vposter=vposter;
		this.vintroduce=vintroduce;
		this.vlikecount=vlikecount;
		this.vsrc=vsrc;
		this.vstate=vstate;
		this.vcountry=vcountry;
		this.vdirector=vdirector;
		this.vcost=vcost;
		this.vstudio=vstudio;
		this.vcheckstate=vcheckstate;
	}
	public void setVid(Integer vid){
		this.vid=vid;
	}
	public Integer getVid(){
		return vid;
	}
	public void setVname(String vname){
		this.vname=vname;
	}
	public String getVname(){
		return vname;
	}
	public void setVtype(Integer vtype){
		this.vtype=vtype;
	}
	public Integer getVtype(){
		return vtype;
	}
	public void setVcategory(Integer vcategory){
		this.vcategory=vcategory;
	}
	public Integer getVcategory(){
		return vcategory;
	}
	public void setVlanguage(Integer vlanguage){
		this.vlanguage=vlanguage;
	}
	public Integer getVlanguage(){
		return vlanguage;
	}
	public void setVuser(Integer vuser){
		this.vuser=vuser;
	}
	public Integer getVuser(){
		return vuser;
	}
	public void setVdate(java.util.Date vdate){
		this.vdate=vdate;
	}
	public java.util.Date getVdate(){
		return vdate;
	}
	public void setVtime(String vtime){
		this.vtime=vtime;
	}
	public String getVtime(){
		return vtime;
	}
	public void setVrating(Double vrating){
		this.vrating=vrating;
	}
	public Double getVrating(){
