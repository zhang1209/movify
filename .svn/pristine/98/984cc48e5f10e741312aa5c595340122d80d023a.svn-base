package com.movify.entity;
public class Actor{
	private Integer aid; // 演员id
	private String aname; // 演员姓名
	private java.util.Date abirthday; // 演员生日
	private Double atall; // 演员身高
	private String aphoto; // 演员图片
	private String acountry; // 演员国籍
	private String asex; // 演员性别
	private String aintroduce; // 演员简介
	public Actor(){}
	public Actor(Integer aid,String aname,java.util.Date abirthday,Double atall,String aphoto,String acountry,String asex,String aintroduce){
		this.aid=aid;
		this.aname=aname;
		this.abirthday=abirthday;
		this.atall=atall;
		this.aphoto=aphoto;
		this.acountry=acountry;
		this.asex=asex;
		this.aintroduce=aintroduce;
	}
	public void setAid(Integer aid){
		this.aid=aid;
	}
	public Integer getAid(){
		return aid;
	}
	public void setAname(String aname){
		this.aname=aname;
	}
	public String getAname(){
		return aname;
	}
	public void setAbirthday(java.util.Date abirthday){
		this.abirthday=abirthday;
	}
	public java.util.Date getAbirthday(){
		return abirthday;
	}
	public void setAtall(Double atall){
		this.atall=atall;
	}
	public Double getAtall(){
		return atall;
	}
	public void setAphoto(String aphoto){
		this.aphoto=aphoto;
	}
	public String getAphoto(){
		return aphoto;
	}
	public void setAcountry(String acountry){
		this.acountry=acountry;
	}
	public String getAcountry(){
		return acountry;
	}
	public void setAsex(String asex){
		this.asex=asex;
	}
	public String getAsex(){
		return asex;
	}
	public void setAintroduce(String aintroduce){
		this.aintroduce=aintroduce;
	}
	public String getAintroduce(){
		return aintroduce;
	}
}